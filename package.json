{
  "name": "materializemodal",
  "version": "1.0.0",
  "description": "Create dynamic modals!",
  "main": "materialize.modal.js",
  "scripts": {
    "compilejs": "./node_modules/.bin/babel materialize.modal.js --out-file materialize.modal.min.js --preset @babel/preset-env",
    "minifyjs": "./node_modules/.bin/minify materialize.modal.js --out-file materialize.modal.min.js",
    "minifycss": "uglifycss materialize.modal.css > materialize.modal.min.css",
    "compile": "npm run compilejs && npm run minifyjs && npm run minifycss",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RaphaelFelten/MaterializeModal.git"
  },
  "keywords": [
    "MaterializeCSS",
    "Modals"
  ],
  "author": "Raphael Felten",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RaphaelFelten/MaterializeModal/issues"
  },
  "homepage": "https://github.com/RaphaelFelten/MaterializeModal#readme",
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "babel-minify": "^0.5.0"
  }
}
